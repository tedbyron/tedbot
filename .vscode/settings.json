{
  "[rust]": {
    "editor.tabSize": 4
  },
  "[toml]": {
    "editor.tabSize": 4
  },
  "evenBetterToml.formatter.allowedBlankLines": 1,
  "evenBetterToml.formatter.columnWidth": 100,
  "evenBetterToml.formatter.trailingNewline": false,
  "evenBetterToml.semanticTokens": true,
  "evenBetterToml.taplo.configFile.enabled": false,
  "files.associations": {
    ".env": "shellscript",
    ".env.example": "shellscript"
  }
}
